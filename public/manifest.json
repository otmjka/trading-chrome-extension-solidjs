{
  "manifest_version": 3,
  "name": "Axiom Trade Extension",
  "version": "1.0.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://gmgn.ai/sol/token/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://gmgn.ai/sol/token/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*.html", "*.js", "*.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
